#import "ColorUtil.h"

#define COLOR(x) x/255.0
@implementation ColorUtil

+(UIColor *) colorLightGrey {
    return [UIColor colorWithRed:COLOR(147.0) green:COLOR(149.0) blue:COLOR(151.0) alpha:1];
}

+(UIColor *) colorDarkGrey {
    return [UIColor colorWithRed:COLOR(64.0) green:COLOR(64.0) blue:COLOR(65.0) alpha:1];
}

@end
