<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.revenuedepartment.app"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/lbTop"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <com.revenuedepartment.customview.TopBar
            android:id="@+id/topbar"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/lbBottom"
        android:layout_below="@+id/lbTop" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_gray_25_bold"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_gray"
                android:gravity="center"
                android:padding="3dp"
                android:text="@string/txInfoPeople" />

            <!-- Start fields -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_countryCode" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txCountryCode"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_buildName" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txBuildName"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_roomNo" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txRoomNo"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_floorNo" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txFloorNo"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_addressNo" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txAddressNo"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_soi" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txSoi"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_village" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txVillage"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_mooNo" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txMooNo"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_street" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txStreet"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_province" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <!--
                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/txProvince"
                        android:hint="@string/lang_th_register_province"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="5dp"
                        android:textColor="@android:color/black" />
                        -->

                        <Spinner
                            android:id="@+id/etProvince"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/lang_th_Label_user_profile_province" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_amphur" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <!--
                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/txAmphur"
                        android:hint="@string/lang_th_register_amphur"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="5dp"
                        android:textColor="@android:color/black" />
                        -->

                        <Spinner
                            android:id="@+id/etAmphur"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/lang_th_Label_user_profile_amphur" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_tambol" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <!--
                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/txTambol"
                        android:hint="@string/lang_th_register_tambol"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="5dp"
                        android:textColor="@android:color/black" />
                        -->

                        <Spinner
                            android:id="@+id/etTambol"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/lang_th_Label_user_profile_tambol" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_marginBottom="10dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/label_black_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/lang_th_register_postcode" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <com.revenuedepartment.customview.EditTextCustom
                            android:id="@+id/txPostcode"
                            style="@style/label_gray_20"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:gravity="right"
                            android:layout_marginTop="5dp"
                            android:maxLength="5" />
                    </RelativeLayout>
                </LinearLayout>
                
                <com.revenuedepartment.customview.ButtonCustom
		            android:id="@+id/btNext"
		            android:layout_marginLeft="20dp"
		            android:layout_marginRight="20dp"
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/txNext" />
                
            </LinearLayout>
            <!-- end fields -->

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/lbBottom"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" >

    </LinearLayout>

</RelativeLayout>