<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.revenuedepartment.app"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <com.revenuedepartment.customview.TopBar
        android:id="@+id/Topbar"
        android:layout_width="fill_parent"
        android:layout_height="55dp"
        android:layout_marginBottom="20dp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Topbar2"
        android:layout_marginTop="20dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
            </LinearLayout>
        </LinearLayout>

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
               
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/rtax_instructions"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/ictax_instructions"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="@dimen/gap_line"
                        android:paddingTop="@dimen/gap_line"
                        android:src="@drawable/icon_suggestion_efiling" />

                    <com.revenuedepartment.customview.TextViewCustom
                        android:id="@+id/txtax_instructions"
                        style="@style/label_gray_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/gap_icon_main_efilling"
                        android:layout_toRightOf="@+id/ictax_instructions"
                        android:text="@string/tvheadtax_instructions"
                        custom:textstyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/right_main_efilling"
                        android:src="@drawable/arrow_next" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="60dp"
                    android:background="@drawable/bg_list_line" />

                <RelativeLayout
                    android:id="@+id/rterm_tax"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/icterm_tax"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="@dimen/gap_line"
                        android:paddingTop="@dimen/gap_line"
                        android:src="@drawable/icon_suggestion_condition" />

                    <com.revenuedepartment.customview.TextViewCustom
                        android:id="@+id/txterm_tax"
                        style="@style/label_gray_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/gap_icon_main_efilling"
                        android:layout_toRightOf="@+id/icterm_tax"
                        android:text="@string/tvheadterm_tax"
                        custom:textstyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/right_main_efilling"
                        android:src="@drawable/arrow_next" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="60dp"
                    android:background="@drawable/bg_list_line" />

                <RelativeLayout
                    android:id="@+id/rindenture_tax"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/icindenture_tax"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="@dimen/gap_line"
                        android:paddingTop="@dimen/gap_line"
                        android:src="@drawable/icon_suggestion_agreement" />

                    <com.revenuedepartment.customview.TextViewCustom
                        android:id="@+id/txindenture_tax"
                        style="@style/label_gray_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/gap_icon_main_efilling"
                        android:layout_toRightOf="@+id/icindenture_tax"
                        android:text="@string/tvheadrindentureTax"
                        custom:textstyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/right_main_efilling"
                        android:src="@drawable/arrow_next" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="60dp"
                    android:background="@drawable/bg_list_line" />

                <RelativeLayout
                    android:id="@+id/rexplanation_tax"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/icexplanation_tax"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="@dimen/gap_line"
                        android:paddingTop="@dimen/gap_line"
                        android:src="@drawable/icon_suggestion_time_extend" />

                    <com.revenuedepartment.customview.TextViewCustom
                        android:id="@+id/txexplanation_tax"
                        style="@style/label_gray_20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/gap_icon_main_efilling"
                        android:layout_toRightOf="@+id/icexplanation_tax"
                        android:text="@string/tvheadrexplanation_tax"
                        custom:textstyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/right_main_efilling"
                        android:src="@drawable/arrow_next" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="60dp"
                    android:background="@drawable/bg_list_line" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <com.revenuedepartment.customview.ButtonCustom
        android:id="@+id/btSetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/icon_setting"
        android:visibility="gone" />

</RelativeLayout>