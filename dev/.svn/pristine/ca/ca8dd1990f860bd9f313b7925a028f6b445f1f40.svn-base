<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.revenuedepartment.app"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/lnTop"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_bar_top"
        android:orientation="horizontal" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"
            android:src="@drawable/icon_efiling"
            android:visibility="invisible" />

        <com.revenuedepartment.customview.TextViewCustom
            android:id="@+id/labelResultCalTax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="30dp"
            android:text="@string/label_cal_tax_th"
            android:textColor="@android:color/white"
            android:textSize="30dp"
            custom:textstyle="bold" />

        <com.revenuedepartment.customview.ButtonCustom
            android:id="@+id/btBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="กลับสู่หน้าหลัก" />

        <com.revenuedepartment.customview.ButtonCustom
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:text="บันทึกหน้าจอ" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lnTop"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="vertical" >

        <com.revenuedepartment.customview.TextViewCustom
            style="@style/label_black_25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/label_result_filling_th"
            custom:textstyle="bold" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_no_filling_th" />

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="SB10555344" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_dateline_filling_th" />

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="03/12/2013" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_25dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="@string/label_info_filling_th"
                custom:textstyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:layout_weight="1"
                android:text="@string/label_no_nid_filling_th" />

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="1234567890123" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_control_th" />

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="2343533532255890" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/label_amount_th" />

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_23dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>

        <com.revenuedepartment.customview.ButtonCustom
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:text="แสดง Barcode" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.ButtonCustom
                style="@style/label_black_25dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ช่องทางการชำระภาษี"
                custom:textstyle="bold" 
                android:id="@+id/btBarCode"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="vertical" >

            <ListView
                android:id="@+id/lvchanall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:animationCache="false"
                android:cacheColorHint="@android:color/transparent"
                android:scrollingCache="false" >
            </ListView>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>