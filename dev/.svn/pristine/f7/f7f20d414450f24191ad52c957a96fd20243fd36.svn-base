                        android:id="@+id/txNid"
                        android:maxLength="17"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/txNid_th"
                        android:text="" >
                    </com.revenuedepartment.customview.TextViewCustom>
                </LinearLayout>

