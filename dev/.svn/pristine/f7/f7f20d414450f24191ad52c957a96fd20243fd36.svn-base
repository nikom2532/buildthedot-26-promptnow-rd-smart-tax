                    android:id="@+id/etTitleName"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/lang_th_Label_user_profile_spouseCountryCode"
                    android:text="" >
                </Spinner>

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/fontTopicProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="@string/lang_th_Label_user_profile_name"
                    custom:textstyle="bold" />

                <com.revenuedepartment.customview.EditTextCustom
                    android:id="@+id/etName"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/lang_th_Label_user_profile_name"
                    android:text="" >
                </com.revenuedepartment.customview.EditTextCustom>
                
                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/fontTopicProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="@string/lang_th_Label_user_profile_middlename"
                    custom:textstyle="bold" />

                <com.revenuedepartment.customview.EditTextCustom
                    android:id="@+id/etMiddleName"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
