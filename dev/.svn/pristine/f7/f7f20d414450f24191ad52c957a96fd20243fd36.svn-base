                            android:hint="@string/lang_th_Label_user_profile_txpMotherPin"
                            android:text="" >
                        </com.revenuedepartment.customview.EditTextCustom>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnNoInCome"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/fontTopicProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="@string/label_txpFatherPin_spouse_th"
                        custom:textstyle="bold" />

                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/etSpouseTxpFatherPin"
                        android:maxLength="17"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/label_txpFatherPin_spouse_th"
                        android:text="" >
                    </com.revenuedepartment.customview.EditTextCustom>

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/fontTopicProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="@string/label_txpMontherPin_spouse_en"
                        custom:textstyle="bold" />

                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/etSpouseTxpMotherPin"
                        android:maxLength="17"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/label_txpMontherPin_spouse_en"
                        android:text="" >
                    </com.revenuedepartment.customview.EditTextCustom>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnchild"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/fontTopicProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="@string/lang_th_Label_user_profile_childNoStudy"
                        custom:textstyle="bold" />

                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/etChildNoStudy"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/lang_th_Label_user_profile_childNoStudy"
                        android:maxLength="2"
                        android:text="" >
                    </com.revenuedepartment.customview.EditTextCustom>

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/fontTopicProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="@string/lang_th_Label_user_profile_childStudy"
                        custom:textstyle="bold" />

                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/etChildStudy"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:maxLength="2"
                        android:hint="@string/lang_th_Label_user_profile_childStudy"
                        android:text="" >
                    </com.revenuedepartment.customview.EditTextCustom>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.ButtonCustom
                    android:id="@+id/btCancel"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:text="@string/label_cancel_th"
                    android:textSize="20dp" />

                <com.revenuedepartment.customview.ButtonCustom
                    android:id="@+id/btSubmit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:text="@string/label_submit_th"
                    android:textSize="20dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>