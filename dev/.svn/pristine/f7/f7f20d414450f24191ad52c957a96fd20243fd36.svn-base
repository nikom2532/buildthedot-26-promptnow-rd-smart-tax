                    android:id="@+id/lnThai"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.revenuedepartment.customview.TextViewCustom
                        style="@style/fontTopicProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="@string/txNid_th"
                        custom:textstyle="bold" />

                    <com.revenuedepartment.customview.EditTextCustom
                        android:id="@+id/etNid"
                        android:maxLength="17"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/txNid_th"
                        android:visibility="gone"
                        android:text="" >
                    </com.revenuedepartment.customview.EditTextCustom>
                    
