<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.revenuedepartment.app"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <com.revenuedepartment.customview.TopBar
        android:id="@+id/Topbar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btSuccess"
        android:layout_below="@+id/Topbar"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="vertical" >

        <com.revenuedepartment.customview.TextViewCustom
            style="@style/label_black_25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/label_result_filling"
            custom:textstyle="bold" />

        <LinearLayout
            android:id="@+id/lnCenter"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/label_no_filling_th" />

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="SB10555344" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/label_dateline_filling_th" />

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="03/12/2013" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_25dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_weight="1"
                    android:text="@string/label_info_filling_th"
                    custom:textstyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:text="@string/label_no_nid_filling_th" />

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="right"
                    android:text="1234567890123" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/label_control_th" />

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="2343533532255890" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/label_amount_th" />

                <com.revenuedepartment.customview.TextViewCustom
                    style="@style/label_black_20dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="" />
            </LinearLayout>
        </LinearLayout>

        <com.revenuedepartment.customview.ButtonCustom
            android:id="@+id/btBarCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:text="แสดง Barcode" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <com.revenuedepartment.customview.TextViewCustom
                style="@style/label_black_25dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ช่องทางการชำระภาษี"
                custom:textstyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="vertical" >

            <ListView
                android:id="@+id/lvchanall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:animationCache="false"
                android:cacheColorHint="@android:color/transparent"
                android:scrollingCache="false" >
            </ListView>
        </LinearLayout>
    </LinearLayout>

    <com.revenuedepartment.customview.ButtonCustom
        android:id="@+id/btSuccess"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:text="@string/label_next" />

</RelativeLayout>