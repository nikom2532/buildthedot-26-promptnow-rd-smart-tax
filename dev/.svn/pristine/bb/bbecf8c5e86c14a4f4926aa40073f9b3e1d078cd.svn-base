package com.promptnow.rdsmart.api.model;

/**
 *
 * @author Promptnow11
 */
public class ConfirmQuestionInputModel extends CommonAPIInputModel{
    public String answer;
    public String questionId;

    public ConfirmQuestionInputModel() {
    }
    public ConfirmQuestionInputModel(String nid, String lang, String authenKey, String answer, String questionId, String version) {
        this.nid = nid;
        this.answer = answer;
        this.questionId = questionId;
        this.version = version;
        this.language = lang;
        this.authenKey = authenKey;
    }
    
    
}
